@model List<MVCWebPresentationLayer.Models.ClienteQueryViewModel>


@{
    ViewBag.Title = "";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Nome</th>
        <th>CPF</th>
        <th>Email</th>
        <th>Data de Nascimento</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Nome</td>
            <td>@item.CPF</td>
            <td>@item.Email</td>
            <td>@item.DataNascimento.ToString("dd/MM/yyyy")</td>
        </tr>
    }

</table>
